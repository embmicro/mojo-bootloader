 251:Caterina.c    **** #if !defined(NO_BLOCK_SUPPORT)
 252:Caterina.c    **** /** Reads or writes a block of EEPROM or FLASH memory to or from the appropriate CDC data endpoint,
 253:Caterina.c    ****  *  on the AVR910 protocol command issued.
 254:Caterina.c    ****  *
 255:Caterina.c    ****  *  \param[in] Command  Single character AVR910 protocol command indicating what memory operation t
 256:Caterina.c    ****  */
 257:Caterina.c    **** static void ReadWriteMemoryBlock(const uint8_t Command)
 258:Caterina.c    **** {
 259:Caterina.c    **** 	uint16_t BlockSize;
 260:Caterina.c    **** 	char     MemoryType;
 261:Caterina.c    **** 
 262:Caterina.c    **** 	bool     HighByte = false;
 263:Caterina.c    **** 	uint8_t  LowByte  = 0;
 264:Caterina.c    **** 
 265:Caterina.c    **** 	BlockSize  = (FetchNextCommandByte() << 8);
 266:Caterina.c    **** 	BlockSize |=  FetchNextCommandByte();
 267:Caterina.c    **** 
 268:Caterina.c    **** 	MemoryType =  FetchNextCommandByte();
 269:Caterina.c    **** 
 270:Caterina.c    **** 	if ((MemoryType != 'E') && (MemoryType != 'F'))
 271:Caterina.c    **** 	{
 272:Caterina.c    **** 		/* Send error byte back to the host */
 273:Caterina.c    **** 		WriteNextResponseByte('?');
 274:Caterina.c    **** 
 275:Caterina.c    **** 		return;
 276:Caterina.c    **** 	}
 277:Caterina.c    **** 
 278:Caterina.c    **** 	/* Disable timer 1 interrupt - can't afford to process nonessential interrupts
 279:Caterina.c    **** 	 * while doing SPM tasks */
 280:Caterina.c    **** 	TIMSK1 = 0;
 281:Caterina.c    **** 
 282:Caterina.c    **** 	/* Check if command is to read memory */
 283:Caterina.c    **** 	if (Command == 'g')
 284:Caterina.c    **** 	{		
 285:Caterina.c    **** 		/* Re-enable RWW section */
 286:Caterina.c    **** 		boot_rww_enable();
 287:Caterina.c    **** 
 288:Caterina.c    **** 		while (BlockSize--)
 289:Caterina.c    **** 		{
 290:Caterina.c    **** 			if (MemoryType == 'F')
 291:Caterina.c    **** 			{
 292:Caterina.c    **** 				/* Read the next FLASH byte from the current FLASH page */
 293:Caterina.c    **** 				#if (FLASHEND > 0xFFFF)
 294:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte_far(CurrAddress | HighByte));
 295:Caterina.c    **** 				#else
 296:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte(CurrAddress | HighByte));
 297:Caterina.c    **** 				#endif
 298:Caterina.c    **** 
 299:Caterina.c    **** 				/* If both bytes in current word have been read, increment the address counter */
 300:Caterina.c    **** 				if (HighByte)
 301:Caterina.c    **** 				  CurrAddress += 2;
 302:Caterina.c    **** 
 303:Caterina.c    **** 				HighByte = !HighByte;
 304:Caterina.c    **** 			}
 305:Caterina.c    **** 			else
 306:Caterina.c    **** 			{
 307:Caterina.c    **** 				/* Read the next EEPROM byte into the endpoint */
 308:Caterina.c    **** 				WriteNextResponseByte(eeprom_read_byte((uint8_t*)(intptr_t)(CurrAddress >> 1)));
 309:Caterina.c    **** 
 310:Caterina.c    **** 				/* Increment the address counter after use */
 311:Caterina.c    **** 				CurrAddress += 2;
 312:Caterina.c    **** 			}
 313:Caterina.c    **** 		}
 314:Caterina.c    **** 	}
 315:Caterina.c    **** 	else
 316:Caterina.c    **** 	{
 317:Caterina.c    **** 		uint32_t PageStartAddress = CurrAddress;
 318:Caterina.c    **** 
 319:Caterina.c    **** 		if (MemoryType == 'F')
 320:Caterina.c    **** 		{
 321:Caterina.c    **** 			boot_page_erase(PageStartAddress);
 322:Caterina.c    **** 			boot_spm_busy_wait();
 323:Caterina.c    **** 		}
 324:Caterina.c    **** 
 325:Caterina.c    **** 		while (BlockSize--)
 326:Caterina.c    **** 		{
 327:Caterina.c    **** 			if (MemoryType == 'F')
 328:Caterina.c    **** 			{
 329:Caterina.c    **** 				/* If both bytes in current word have been written, increment the address counter */
 330:Caterina.c    **** 				if (HighByte)
 331:Caterina.c    **** 				{
 332:Caterina.c    **** 					/* Write the next FLASH word to the current FLASH page */
 333:Caterina.c    **** 					boot_page_fill(CurrAddress, ((FetchNextCommandByte() << 8) | LowByte));
 334:Caterina.c    **** 
 335:Caterina.c    **** 					/* Increment the address counter after use */
 336:Caterina.c    **** 					CurrAddress += 2;
 337:Caterina.c    **** 				}
 338:Caterina.c    **** 				else
 339:Caterina.c    **** 				{
 340:Caterina.c    **** 					LowByte = FetchNextCommandByte();
 341:Caterina.c    **** 				}
 342:Caterina.c    **** 				
 343:Caterina.c    **** 				HighByte = !HighByte;
 344:Caterina.c    **** 			}
 345:Caterina.c    **** 			else
 346:Caterina.c    **** 			{
 347:Caterina.c    **** 				/* Write the next EEPROM byte from the endpoint */
 348:Caterina.c    **** 				eeprom_write_byte((uint8_t*)((intptr_t)(CurrAddress >> 1)), FetchNextCommandByte());
 349:Caterina.c    **** 
 350:Caterina.c    **** 				/* Increment the address counter after use */
 351:Caterina.c    **** 				CurrAddress += 2;
 352:Caterina.c    **** 			}
 353:Caterina.c    **** 		}
 354:Caterina.c    **** 
 355:Caterina.c    **** 		/* If in FLASH programming mode, commit the page after writing */
 356:Caterina.c    **** 		if (MemoryType == 'F')
 357:Caterina.c    **** 		{
 358:Caterina.c    **** 			/* Commit the flash page to memory */
 359:Caterina.c    **** 			boot_page_write(PageStartAddress);
 360:Caterina.c    **** 
 361:Caterina.c    **** 			/* Wait until write operation has completed */
 362:Caterina.c    **** 			boot_spm_busy_wait();
 363:Caterina.c    **** 		}
 364:Caterina.c    **** 
 365:Caterina.c    **** 		/* Send response byte back to the host */
 366:Caterina.c    **** 		WriteNextResponseByte('\r');
 367:Caterina.c    **** 	}
 368:Caterina.c    **** 
 369:Caterina.c    **** 	/* Re-enable timer 1 interrupt disabled earlier in this routine */	
 370:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);
 371:Caterina.c    **** }
 372:Caterina.c    **** #endif

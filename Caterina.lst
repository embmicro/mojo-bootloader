  76:Caterina.c    **** void StartSketch(void)
  77:Caterina.c    **** {
  78:Caterina.c    **** 	cli();
  79:Caterina.c    **** 	
  80:Caterina.c    **** 	/* Undo TIMER1 setup and clear the count before running the sketch */
  81:Caterina.c    **** 	TIMSK1 = 0;
  82:Caterina.c    **** 	TCCR1B = 0;
  83:Caterina.c    **** 	TCNT1H = 0;		// 16-bit write to TCNT1 requires high byte be written first
  84:Caterina.c    **** 	TCNT1L = 0;
  85:Caterina.c    **** 	
  86:Caterina.c    **** 	/* Relocate the interrupt vector table to the application section */
  87:Caterina.c    **** 	MCUCR = (1 << IVCE);
  88:Caterina.c    **** 	MCUCR = 0;
  89:Caterina.c    **** 
  90:Caterina.c    **** 	L_LED_OFF();
  91:Caterina.c    **** 	TX_LED_OFF();
  92:Caterina.c    **** 	RX_LED_OFF();
  93:Caterina.c    **** 
  94:Caterina.c    **** 	/* jump to beginning of application space */
  95:Caterina.c    **** 	__asm__ volatile("jmp 0x0000");
  96:Caterina.c    **** }

 149:Caterina.c    **** /** Configures all hardware required for the bootloader. */
 150:Caterina.c    **** void SetupHardware(void)
 151:Caterina.c    **** {
 152:Caterina.c    **** 	/* Disable watchdog if enabled by bootloader/fuses */
 153:Caterina.c    **** 	MCUSR &= ~(1 << WDRF);
 154:Caterina.c    **** 	wdt_disable();
 155:Caterina.c    **** 
 156:Caterina.c    **** 	/* Disable clock division */
 157:Caterina.c    **** 	clock_prescale_set(clock_div_1);
 158:Caterina.c    **** 
 159:Caterina.c    **** 	/* Relocate the interrupt vector table to the bootloader section */
 160:Caterina.c    **** 	MCUCR = (1 << IVCE);
 161:Caterina.c    **** 	MCUCR = (1 << IVSEL);
 162:Caterina.c    **** 	
 163:Caterina.c    **** 	CPU_PRESCALE(0); 
 164:Caterina.c    **** 	
 165:Caterina.c    **** 	/* Initialize TIMER1 to handle bootloader timeout and LED tasks.  
 166:Caterina.c    **** 	 * With 16 MHz clock and 1/64 prescaler, timer 1 is clocked at 250 kHz
 167:Caterina.c    **** 	 * Our chosen compare match generates an interrupt every 1 ms.
 168:Caterina.c    **** 	 * This interrupt is disabled selectively when doing memory reading, erasing,
 169:Caterina.c    **** 	 * or writing since SPM has tight timing requirements.
 170:Caterina.c    **** 	 */ 
 171:Caterina.c    **** 	OCR1AH = 0;
 172:Caterina.c    **** 	OCR1AL = 250;
 173:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);					// enable timer 1 output compare A match interrupt
 174:Caterina.c    **** 	TCCR1B = ((1 << CS11) | (1 << CS10));	// 1/64 prescaler on timer 1 input
 175:Caterina.c    **** 
 176:Caterina.c    **** 	/* Initialize USB Subsystem */
 177:Caterina.c    **** 	USB_Init();
 178:Caterina.c    **** }

 210:Caterina.c    **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 211:Caterina.c    ****  *  the device from the USB host before passing along unhandled control requests to the library for
 212:Caterina.c    ****  *  internally.
 213:Caterina.c    ****  */
 214:Caterina.c    **** void EVENT_USB_Device_ControlRequest(void)
 215:Caterina.c    **** {
 216:Caterina.c    **** 	/* Ignore any requests that aren't directed to the CDC interface */
 217:Caterina.c    **** 	if ((USB_ControlRequest.bmRequestType & (CONTROL_REQTYPE_TYPE | CONTROL_REQTYPE_RECIPIENT)) !=
 218:Caterina.c    **** 	    (REQTYPE_CLASS | REQREC_INTERFACE))
 219:Caterina.c    **** 	{
 220:Caterina.c    **** 		return;
 221:Caterina.c    **** 	}
 222:Caterina.c    **** 
 223:Caterina.c    **** 	/* Process CDC specific control requests */
 224:Caterina.c    **** 	switch (USB_ControlRequest.bRequest)
 225:Caterina.c    **** 	{
 226:Caterina.c    **** 		case CDC_REQ_GetLineEncoding:
 227:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_INTERFACE)
 228:Caterina.c    **** 			{
 229:Caterina.c    **** 				Endpoint_ClearSETUP();
 230:Caterina.c    **** 
 231:Caterina.c    **** 				/* Write the line coding data to the control endpoint */
 232:Caterina.c    **** 				Endpoint_Write_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 233:Caterina.c    **** 				Endpoint_ClearOUT();
 234:Caterina.c    **** 			}
 235:Caterina.c    **** 
 236:Caterina.c    **** 			break;
 237:Caterina.c    **** 		case CDC_REQ_SetLineEncoding:
 238:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_INTERFACE)
 239:Caterina.c    **** 			{
 240:Caterina.c    **** 				Endpoint_ClearSETUP();
 241:Caterina.c    **** 
 242:Caterina.c    **** 				/* Read the line coding data in from the host into the global struct */
 243:Caterina.c    **** 				Endpoint_Read_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 244:Caterina.c    **** 				Endpoint_ClearIN();
 245:Caterina.c    **** 			}
 246:Caterina.c    **** 
 247:Caterina.c    **** 			break;
 248:Caterina.c    **** 	}
 249:Caterina.c    **** }

  98:Caterina.c    **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
  99:Caterina.c    ****  *  runs the bootloader processing routine until it times out or is instructed to exit.
 100:Caterina.c    ****  */
 101:Caterina.c    **** int main(void)
 102:Caterina.c    **** {
 103:Caterina.c    **** 	/* Save the value of the boot key memory before it is overwritten */
 104:Caterina.c    **** 	uint16_t bootKeyPtrVal = *bootKeyPtr;
 105:Caterina.c    **** 	*bootKeyPtr = 0;
 106:Caterina.c    **** 
 107:Caterina.c    **** 	/* Check the reason for the reset so we can act accordingly */
 108:Caterina.c    **** 	uint8_t  mcusr_state = MCUSR;		// store the initial state of the Status register
 109:Caterina.c    **** 	MCUSR = 0;							// clear all reset flags	
 110:Caterina.c    **** 
 111:Caterina.c    **** 	/* Watchdog may be configured with a 15 ms period so must disable it before going any further */
 112:Caterina.c    **** 	wdt_disable();
 113:Caterina.c    **** 	
 114:Caterina.c    **** 	if (mcusr_state & (1<<EXTRF)) {
 115:Caterina.c    **** 		// External reset -  we should continue to self-programming mode.
 116:Caterina.c    **** 	} else if ((mcusr_state & (1<<PORF)) && (pgm_read_word(0) != 0xFFFF)) {		
 117:Caterina.c    **** 		// After a power-on reset skip the bootloader and jump straight to sketch 
 118:Caterina.c    **** 		// if one exists.	
 119:Caterina.c    **** 		StartSketch();
 120:Caterina.c    **** 	} else if ((mcusr_state & (1<<WDRF)) && (bootKeyPtrVal != bootKey) && (pgm_read_word(0) != 0xFFFF)
 121:Caterina.c    **** 		// If it looks like an "accidental" watchdog reset then start the sketch.
 122:Caterina.c    **** 		StartSketch();
 123:Caterina.c    **** 	}
 124:Caterina.c    **** 	
 125:Caterina.c    **** 	/* Setup hardware required for the bootloader */
 126:Caterina.c    **** 	SetupHardware();
 127:Caterina.c    **** 
 128:Caterina.c    **** 	/* Enable global interrupts so that the USB stack can function */
 129:Caterina.c    **** 	sei();
 130:Caterina.c    **** 	
 131:Caterina.c    **** 	Timeout = 0;
 132:Caterina.c    **** 	
 133:Caterina.c    **** 	while (RunBootloader)
 134:Caterina.c    **** 	{
 135:Caterina.c    **** 		CDC_Task();
 136:Caterina.c    **** 		USB_USBTask();
 137:Caterina.c    **** 		/* Time out and start the sketch if one is present */
 138:Caterina.c    **** 		if (Timeout > TIMEOUT_PERIOD)
 139:Caterina.c    **** 			RunBootloader = false;
 140:Caterina.c    **** 	}
 141:Caterina.c    **** 
 142:Caterina.c    **** 	/* Disconnect from the host - USB interface will be reset later along with the AVR */
 143:Caterina.c    **** 	USB_Detach();
 144:Caterina.c    **** 
 145:Caterina.c    **** 	/* Jump to beginning of application space to run the sketch - do not reset */	
 146:Caterina.c    **** 	StartSketch();
 147:Caterina.c    **** }
